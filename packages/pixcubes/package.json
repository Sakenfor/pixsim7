{
  "name": "@pixsim7/pixcubes",
  "version": "0.1.0",
  "private": true,
  "description": "Reusable 3D cube UI system with formations, drag behavior, and panel minimization",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "typescript": "^5.9.3",
    "zustand": "^5.0.9"
  },
  "peerDependencies": {
    "clsx": ">=2",
    "react": ">=18",
    "react-dom": ">=18",
    "zustand": ">=4"
  }
}
