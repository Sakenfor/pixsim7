{
  "name": "@pixsim7/capabilities-core",
  "version": "0.0.1",
  "private": true,
  "description": "Core capability registry - pure TypeScript, no React/DOM dependencies",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./provider": {
      "import": "./src/provider/index.ts",
      "require": "./src/provider/index.ts",
      "types": "./src/provider/index.ts"
    },
    "./app": {
      "import": "./src/app/index.ts",
      "require": "./src/app/index.ts",
      "types": "./src/app/index.ts"
    }
  },
  "files": ["src", "dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@pixsim7/helpers-core": "workspace:*",
    "@pixsim7/shared.types": "workspace:*"
  },
  "peerDependencies": {},
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
