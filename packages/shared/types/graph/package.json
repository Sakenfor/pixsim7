{
  "name": "@pixsim7/shared.types.graph",
  "version": "0.0.1",
  "private": true,
  "description": "Canonical graph schema and runtime for PixSim7 node execution",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": ["src", "dist", "schema"],
  "scripts": {
