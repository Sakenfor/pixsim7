    "test": "vitest"
  },
  "dependencies": {
    "zod": "^3.23.8",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "tsx": "^4.7.0",
    "vitest": "^1.3.0",
    "@types/node": "^20.11.0"
  }
}
