{
  "name": "@pixsim7/admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.3",
    "vite": "^7.2.2"
  },
  "pixsim": {
    "service": {
      "id": "admin",
      "type": "frontend",
      "name": "Admin Console",
      "description": "Launcher admin UI",
      "directory": "apps/admin",
      "command": "pnpm",
      "args": ["dev", "--port"],
      "port_env": "ADMIN_PORT",
      "default_port": 5175,
      "base_url_env": "ADMIN_BASE_URL",
      "env_overrides": {
        "VITE_API_URL": "$LAUNCHER_BASE_URL"
      },
      "auto_start": false,
      "enabled": true
    }
  }
}
