{
  "name": "@pixsim7/devtools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild": "pnpm run build:deps",
    "build:deps": "pnpm --filter @pixsim7/shared.types build && pnpm --filter @pixsim7/shared.ui build && pnpm --filter @pixsim7/game.engine build && pnpm --filter @pixsim7/game.components build && pnpm --filter @pixsim7/scene.shapes build && pnpm --filter @pixsim7/scene.gizmos build",
    "build:check": "tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@pixsim7/shared.api-client": "workspace:*",
    "@pixsim7/shared.capabilities.core": "workspace:*",
    "@pixsim7/graphics.geometry": "workspace:*",
    "@pixsim7/shared.ref.core": "workspace:*",
    "@pixsim7/game.components": "workspace:*",
    "@pixsim7/game.engine": "workspace:*",
    "@pixsim7/game.react": "workspace:*",
    "@pixsim7/core.composer": "workspace:*",
    "@pixsim7/shared.helpers.core": "workspace:*",
    "@pixsim7/scene.gizmos": "workspace:*",
    "@pixsim7/scene.shapes": "workspace:*",
    "@pixsim7/shared.assets.core": "workspace:*",
    "@pixsim7/shared.generation.core": "workspace:*",
    "@pixsim7/shared.types": "workspace:*",
    "@pixsim7/shared.ui": "workspace:*",
    "@pixsim7/pixcubes": "workspace:*",
    "@react-spring/three": "^10.0.3",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dockview": "^4.11.0",
    "dockview-core": "^4.11.0",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-draggable": "^4.5.0",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^7.9.5",
    "reactflow": "^11.11.4",
    "three": "^0.181.2",
    "zundo": "^2.3.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@pixsim7/shared.config": "workspace:*",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-react": "^5.1.0",
    "vite-tsconfig-paths": "^5.0.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.3",
    "vite": "^7.2.2"
  },
  "pixsim": {
    "service": {
      "id": "devtools",
      "type": "frontend",
      "name": "DevTools",
      "description": "Developer tools workspace",
      "directory": "apps/devtools",
      "command": "pnpm",
      "args": ["dev", "--port"],
      "port_env": "DEVTOOLS_PORT",
      "default_port": 5176,
      "base_url_env": "DEVTOOLS_BASE_URL",
      "subdomain": "dev",
      "env_overrides": {
        "VITE_BACKEND_URL": "$BACKEND_BASE_URL"
      },
      "auto_start": false,
      "enabled": true
    }
  }
}
